{
  "name": "sentinel-scanner",
  "description": "[WIP] An open-source web app vulnerability scanner developed by Rebackk.",
  "version": "0.0.0-development",
  "exports": "./build/index.js",
  "types": "./build/index.d.ts",
  "bin": "./build/bin.js",
  "type": "module",
  "main": "./build/index.js",
  "license": "Apache-2.0",
  "engines": {
    "node": "^22.11.0",
    "npm": "^10.5.0"
  },
  "volta": {
    "node": "^22.11.0",
    "npm": "^10.5.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "node --disable-warning=ExperimentalWarning --experimental-strip-types  ./scripts/build.ts",
    "clean": "rimraf build coverage",
    "type:check": "tsc --noEmit",
    "lint": "biome check . --write --unsafe",
    "lint:check": "biome ci .",
    "test": "npm run build && ava src/__tests__/*.ts",
    "test:watch": "npm run build && ava src/__tests__/*.ts --watch",
    "test:coverage": "npm run build && ava src/__tests__/*.ts --coverage",
    "spell:check": "cspell \"{DISCLAIMER.md,README.md,CODE_OF_CONDUCT.md,CONTRIBUTING.md,.github/*.md,src/**/*.ts}\"",
    "cz": "cz",
    "semantic-release": "semantic-release",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@microsoft/api-extractor": "^7.47.11",
    "@ryansonshine/commitizen": "^4.2.8",
    "@ryansonshine/cz-conventional-changelog": "^3.3.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/github": "^10.3.5",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/axios": "^0.9.36",
    "@types/isomorphic-fetch": "^0.0.39",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.9.0",
    "@types/prompts": "^2.4.9",
    "@types/puppeteer": "^7.0.4",
    "@types/user-agents": "^1.0.4",
    "@types/yargs": "^17.0.33",
    "ae-cvss-calculator": "^1.0.2",
    "ava": "^6.2.0",
    "c8": "^10.1.2",
    "cspell": "^8.15.7",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.24.0",
    "husky": "^9.1.6",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.2.0",
    "threads": "^1.7.0",
    "tiny-worker": "^2.3.0",
    "typescript": "^5.4.2",
    "winston": "^3.17.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "jsdom": "^25.0.1",
    "puppeteer": "^23.8.0",
    "user-agents": "^1.1.359",
    "yargs": "^17.7.2"
  }
}
